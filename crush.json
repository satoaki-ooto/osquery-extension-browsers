{
  "$schema": "https://charm.land/crush.json",
  "lsp": {
    "Go": {
      "command": "gopls"
    }
  },
  "mcp": {
    "container-use": {
      "type": "stdio",
      "command": "cu",
      "args": ["stdio"]
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ]
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "deepwiki": {
      "type": "http",
      "url": "https://mcp.deepwiki.com/mcp"
    }
  }
}
